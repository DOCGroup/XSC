project : vcwarnings {
  requires += xsc

  specific {
    xsc_bin   = $(XSC_ROOT)/bin/xsc
    xsc_dep   = $(XSC_ROOT)/bin/xsc
  }

  Define_Custom(XSC) {
    automatic         = 0
    dependent         = $(XSC_DEP)
    command           = $(XSC_BIN)
    inputext          = .xsd
    keyword xscflags  = commandflags
  }
}
